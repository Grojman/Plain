<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="LightModeBackground">#E5EAF5</Color>
    <Color x:Key="DarkModeBackground">#514D5F</Color>
    <Color x:Key="LightModeShadowColor">#D2D0BC</Color>
    <Color x:Key="DarkModeShadowColor">#FFFDDE</Color>
    <Color x:Key="TitleLightForeground">#28214E</Color>
    <Color x:Key="TitleDarkForeground">#BBAEFF</Color>
    <Color x:Key="TextLightForeground">#433683</Color>
    <Color x:Key="TextDarkForeground">#A69AE6</Color>
    
    
    <FontFamily x:Key="TitleFont">pack://application:,,,/Resources/Fonts/#Jaapokki enchance OpenType</FontFamily>
    <FontFamily x:Key="TextFont">pack://application:,,,/Resources/Fonts/#Orkney Medium</FontFamily>
</ResourceDictionary>